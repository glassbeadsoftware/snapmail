{
  "name": "Snapmail",
  "version": "0.0.4",
  "description": "A minimalist email application on holochain",
  "main": "src/main.js",
  "scripts": {
    "start": "./node_modules/.bin/electron electron",
    "alex": "./node_modules/.bin/electron . alex --no-sandbox",
    "billy": "./node_modules/.bin/electron electron billy --no-sandbox",
    "camille": "./node_modules/.bin/electron electron camille --no-sandbox",
    "start-no": "./node_modules/.bin/electron electron --no-sandbox --enable-logging",
    "package-windows": "./node_modules/.bin/electron-packager electron Snapmail --platform=win32 --out=../build --overwrite",
    "package-osx": "./node_modules/.bin/electron-packager electron Snapmail --platform=darwin --arch=x64 --out=../build --overwrite",
    "package-mac": "./node_modules/.bin/electron-packager electron Snapmail --platform=darwin --arch=x64 --prune=true --overwrite --out=../build",
    "package-linux-asar": "./node_modules/.bin/electron-packager electron Snapmail --platform=linux --arch=x64 --asar --prune=true --overwrite --out=../build",
    "package-linux": "./node_modules/.bin/electron-packager electron Snapmail --platform=linux --arch=x64 --overwrite --out=../build",
    "package-arm": "./node_modules/.bin/electron-packager electron Snapmail --platform=linux --arch=arm64 --overwrite --out=../build"
  },
  "repository": "https://github.com/glassbeadsoftware/snapmail-release",
  "keywords": [
    "Electron",
    "Holochain",
    "Snapmail"
  ],
  "author": "Harris-Braun Enterprises, LLC and contributors",
  "license": "CAL-1.0 Beta 4",
  "dependencies": {
    "@holochain/conductor-api": "=0.0.3",
    "byte-base64": "^1.1.0",
    "command-exists": "^1.2.8",
    "dialogs": "^2.0.1",
    "electron-context-menu": "^0.12.1",
    "electron-debug": "^3.0.1",
    "electron-log": "^3.0.9",
    "electron-osx-sign": "^0.4.14",
    "electron-packager": "^14.1.0",
    "electron-prompt": "^1.6.0",
    "fix-path": "^2.1.0",
    "lint": "^0.7.0",
    "open": "^6.3.0",
    "request": "^2.88.2",
    "smalltalk": "^4.0.6",
    "sudo-prompt": "^9.0.0",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "electron": "^8.5.5"
  }
}
